{
  "version": 2,
  "builds": [
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://nodejs-production-da51.up.railway.app/api/$1"
    },
    {
      "src": "/",
      "dest": "/client/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
